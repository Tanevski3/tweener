{
  "name": "@mtanevski/tweener",
  "version": "0.0.3",
  "description": "Tiny library for animating javascript objects in the browser using tween technology",
  "private": false,
  "scripts": {
    "build": "webpack --config webpack.dev.js",
    "watch": "webpack --config webpack.dev.js --watch --info-verbosity verbose",
    "serve": "webpack-dev-server --config webpack.dev.js",
    "build:prod": "rm -rf dist/ && webpack -p --config webpack.prod.js"
  },
  "main": "dist/tweener.js",
  "types": "dist/src/",
  "files": [
    "/dist/src/*"
  ],
  "keywords": [
    "small",
    "tween",
    "animation",
    "typescript",
    "webpack"
  ],
  "author": "marjantanevski@outlook.com",
  "license": "MIT",
  "devDependencies": {
    "@webpack-cli/init": "^0.1.2",
    "babel-core": "^6.26.3",
    "babel-loader": "^7.1.5",
    "babel-plugin-syntax-dynamic-import": "^6.18.0",
    "babel-preset-env": "^1.7.0",
    "css-loader": "^1.0.1",
    "html-webpack-plugin": "^3.2.0",
    "replace-in-file-webpack-plugin": "^1.0.6",
    "style-loader": "^0.23.1",
    "ts-loader": "^5.3.0",
    "typescript": "^3.1.6",
    "webpack": "^4.25.1",
    "webpack-cli": "^3.1.2",
    "webpack-dev-server": "^3.2.1",
    "webpack-merge": "^4.2.2"
  }
}
